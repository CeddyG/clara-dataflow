{
    "name": "ceddyg/clara-dataflow",
    "description": "Repository to create dataflow from the database using query builder.",
    "keywords": ["laravel", "clara", "dataflow", "entity", "xml", "csv", "xls", "export", "import"],
    "authors": [
        {
            "name": "CeddyG"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0",
        "maatwebsite/excel": "^3.1",
        "ceddyg/query-builder-repository": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "CeddyG\\ClaraDataflow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CeddyG\\ClaraDataflow\\DataflowServiceProvider"
            ],
            "aliases": {
                "Dataflow": "CeddyG\\ClaraDataflow\\Facades\\Dataflow"
            }
        }
    }
}
